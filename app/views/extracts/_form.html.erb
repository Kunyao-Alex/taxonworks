<%= content_tag(:span, 'An new extract task exists and should be used.', class: [:feedback, 'feedback-warning']) %> <%= link_to('New extract task', new_extract_task_path) %></p>


<%= form_for(@extract) do |f| %>
    <%= render 'shared/errors', object: @extract %>

    <div class="required">
      <div class="field">
        <%= f.label :verbatim_anatomical_origin %>
        <br>
        <%= f.text_field :verbatim_anatomical_origin %>
      </div>

      <div class="field">
        <%= f.label :year_made %>
        <br>
        <%= f.text_field :year_made %>
      </div>

      <div class="field">
        <%= f.label :month_made %>
        <br>
        <%= f.text_field :month_made %>
      </div>

      <div class="field">
        <%= f.label :day_made %>
        <br>
        <%= f.text_field :day_made %>
      </div>
    </div>

    <div class="actions">
      <%= f.submit %>
    </div>
<% end %>
